#!/bin/bash

issue=$(git cl issue 2>&1 | sed -n 's/^[^0-9]*\([0-9]*\).*$/\1/p')
if test $? -ne 0
then
  echo "Can't get an issue number" 1>&2
  exit 1
fi

tmp=/tmp/Review.html.$$
url="https://codereview.chromium.org/rss/issue/${issue}"
trap "rm -f $tmp" 0 1 2 15

if wget -q --output-document=$tmp "$url"
then
  cat $tmp
fi
